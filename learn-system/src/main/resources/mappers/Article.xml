<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.learn.system.mapper.ArticleMapper">
    <select id="getArticle" resultType="pers.learn.system.entity.Article">
    SELECT
        *
        FROM `article`
        LIMIT 1
    </select>
</mapper>